{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node-terminal",
            "request": "launch",
            "name": "Docker-Compose Up",
            "command": "docker-compose -f docker-compose.dev.yml up"
        },
        {
            "type": "node-terminal",
            "request": "launch",
